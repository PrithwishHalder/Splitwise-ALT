<Analysis>
    id: "analysis"

    MDBoxLayout:

    MDBottomNavigation:
        #panel_color: "#eeeaea"
        selected_color_background: "orange"
        text_color_active: "lightgrey"

        MDBottomNavigationItem:
            name: 'home'
            text: 'Home'
            icon: 'home-outline'
            badge_icon: ""
            on_tab_press: app.navigate('home')

        MDBottomNavigationItem:
            name: 'account'
            text: 'Account'
            icon: 'book-account-outline'
            badge_icon: ""
            on_tab_press: app.navigate('account')

        MDBottomNavigationItem:
            name: 'analysis'
            text: 'Analysis'
            icon: 'chart-donut'
            badge_icon: ""
            on_tab_press: app.navigate('analysis')

            MDLabel:
                text: root.label_data
                halign: 'center'

        MDBottomNavigationItem:
            name: 'profile'
            text: 'Profile'
            icon: 'account-outline'
            on_tab_press: app.navigate('profile')